		"description": "Configure the bundled plugins.",
		"type": "section",
		"children": [
			{
				"key": "prettier.plugins.prettier-plugin-php.section",
				"title": "@prettier/plugin-php",
				"description": "PHP formatter plugin for Prettier",
				"link": "https://github.com/prettier/plugin-php",
				"type": "section",
				"children": [
					{
						"key": "prettier.plugins.prettier-plugin-php.enabled",
						"title": "Plugin Enabled",
						"type": "boolean",
						"default": true
					},
					{
						"key": "prettier.plugins.prettier-plugin-php.phpVersion",
						"title": "PHP Version",
						"description": "Allows specifying the PHP version you're using. If you're using PHP 7.1 or later, setting this option will make use of modern language features in the printed output. If you're using PHP lower than 7.0, you'll have to set this option or Prettier will generate incompatible code.",
						"type": "enum",
						"default": "7.0",
						"values": [
							"5.0",
							"5.1",
							"5.2",
							"5.3",
							"5.4",
							"5.5",
							"5.6",
							"7.0",
							"7.1",
							"7.2",
							"7.3",
							"7.4",
							"8.0",
							"8.1",
							"8.2"
						]
					},
					{
						"key": "prettier.plugins.prettier-plugin-php.singleQuote",
						"title": "Single Quote",
						"description": "If set to true, strings that use double quotes but do not rely on the features they add, will be reformatted.",
						"type": "boolean",
						"default": false
					},
					{
						"key": "prettier.plugins.prettier-plugin-php.trailingCommaPHP",
						"title": "Trailing Comma",
						"description": "If set to true, trailing commas will be added wherever possible. If set to false, no trailing commas are printed.",
						"type": "boolean",
						"default": true
					},
					{
						"key": "prettier.plugins.prettier-plugin-php.braceStyle",
						"title": "Brace Style",
						"description": "If set to 'per-cs', prettier will move open brace for code blocks (classes, functions and methods) onto new line. If set to '1tbs', prettier will move open brace for code blocks (classes, functions and methods) onto same line.",
						"type": "enum",
						"radio": false,
						"default": "per-cs",
						"values": ["per-cs", "1tbs"]
					}
				]
			},
			{
